int Setup::RNG(void){

	int number = 0;
	int numberOfCoords = 6; //Hardcoded value

	/* initialize random seed: */
	  srand (time(NULL));

	  /* generate number between 1 and numberOfCoords: */
	  number = rand() % numberOfCoords + 1;

	  return number;
}
