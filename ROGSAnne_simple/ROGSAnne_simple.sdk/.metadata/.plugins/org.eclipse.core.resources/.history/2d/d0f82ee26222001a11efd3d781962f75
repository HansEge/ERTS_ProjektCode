#define POPULATION_SIZE 50

#ifndef POPULATION_H_
#define POPULATION_H_

#include "Chromosome.h"

class Population {
public:
	Population();
	virtual ~Population();

	Chromosome chromosomes[POPULATION_SIZE];
	double fitnesses[POPULATION_SIZE];
	double distances[POPULATION_SIZE];

};

#endif /* POPULATION_H_ */
