#ifndef BASESTATE_H_
#define BASESTATE_H_

class BaseState {
public:
	BaseState();
	virtual ~BaseState();

	void SetupDone();
	void FitnessCalculated();
	void StopConditionMet();
	void StopConditionNotMet();
	void NewGenerationReady();
};

#endif /* BASESTATE_H_ */
